<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.local_syogi">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.example.local_syogi.di.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="com.example.local_syogi.presentation.view.game.GameRateActivity"></activity>
        <activity android:name="com.example.local_syogi.presentation.view.setting.SelectNormalFragment" />
        <activity android:name="com.example.local_syogi.syogibase.presentation.view.WinLoseModal" />
        <activity android:name="com.example.local_syogi.syogibase.presentation.view.GameActivity" />
        <activity android:name="com.example.local_syogi.presentation.view.account_information.SettingAccountFragment"/>
        <activity android:name=".presentation.view.setting.SettingActivity" />
        <activity android:name="com.example.local_syogi.presentation.view.game.MattingActivity" />
        <activity android:name=".presentation.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>